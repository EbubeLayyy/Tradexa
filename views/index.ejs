    <script>
      document.addEventListener("DOMContentLoaded", function () {
        const bgImages = document.querySelectorAll(
          ".hero-background-images .bg-image"
        );
        let currentImageIndex = 0;

        const heroBackgrounds = ["./images/back3.jpg", "./images/back8.jpg"];

        if (bgImages.length > 0 && heroBackgrounds.length > 0) {
          bgImages[0].style.backgroundImage = `url('${heroBackgrounds[0]}')`;
          bgImages[0].classList.add("active");
        }

        function changeBackgroundImage() {
          bgImages[currentImageIndex].classList.remove("active");
          currentImageIndex = (currentImageIndex + 1) % heroBackgrounds.length;
          bgImages[
            currentImageIndex
          ].style.backgroundImage = `url('${heroBackgrounds[currentImageIndex]}')`;
          bgImages[currentImageIndex].classList.add("active");
        }

        if (heroBackgrounds.length > 1) {
          setInterval(changeBackgroundImage, 8000);
        }

        // 1. Get all testimonial elements
        const testimonials = document.querySelectorAll(".testimonial");

        // 2. Get references to the arrow buttons
        const prevArrow = document.querySelector(".prev-arrow");
        const nextArrow = document.querySelector(".next-arrow");

        // Initialize the current testimonial index
        let testimonialIndex = 0;

        // Variable to hold the interval ID, so we can clear it
        let slideInterval;

        // Function to update the display of testimonials
        function showTestimonial(index) {
          // Remove 'testimonial-active' from all testimonials
          testimonials.forEach((testimonial) => {
            testimonial.classList.remove("testimonial-active");
          });

          // Add 'testimonial-active' to the current testimonial
          testimonials[index].classList.add("testimonial-active");
        }

        // Function to slide to the next testimonial (for automatic and next button)
        function slideNext() {
          // Hide the current active testimonial
          testimonials[testimonialIndex].classList.remove("testimonial-active");

          // Move to the next index, looping back to the start if at the end
          testimonialIndex = (testimonialIndex + 1) % testimonials.length;

          // Show the new active testimonial
          testimonials[testimonialIndex].classList.add("testimonial-active");
        }

        // Function to slide to the previous testimonial (for prev button)
        function slidePrev() {
          // Hide the current active testimonial
          testimonials[testimonialIndex].classList.remove("testimonial-active");

          // Move to the previous index, looping to the end if at the start
          testimonialIndex =
            (testimonialIndex - 1 + testimonials.length) % testimonials.length;

          // Show the new active testimonial
          testimonials[testimonialIndex].classList.add("testimonial-active");
        }

        // Function to start the automatic slideshow
        function startAutoSlide() {
          // Clear any existing interval to prevent multiple intervals running
          clearInterval(slideInterval);
          slideInterval = setInterval(slideNext, 4000); // 4 seconds
        }

        // Add event listeners for the arrows
        if (prevArrow && nextArrow) {
          prevArrow.addEventListener("click", () => {
            slidePrev();
            startAutoSlide(); // Reset the auto-slide timer after manual interaction
          });

          nextArrow.addEventListener("click", () => {
            slideNext();
            startAutoSlide(); // Reset the auto-slide timer after manual interaction
          });
        } else {
          console.error(
            "Error: Testimonial arrow buttons not found. Check HTML classes."
          );
        }

        // Initial display and start the automatic slideshow
        if (testimonials.length > 0) {
          showTestimonial(testimonialIndex); // Ensure the first testimonial is active on load
          startAutoSlide();
        } else {
          console.warn(
            "No testimonials found. Testimonial slider will not function."
          );
        }

        const faqButtons = document.querySelectorAll(".faq-question");

        faqButtons.forEach((btn) => {
          btn.addEventListener("click", () => {
            btn.classList.toggle("active");

            const answer = btn.nextElementSibling;
            const icon = btn.querySelector(".icon");

            if (btn.classList.contains("active")) {
              answer.style.maxHeight = answer.scrollHeight + "px";
            } else {
              answer.style.maxHeight = null;
            }
          });
        });

        const scrollRevealElements =
          document.querySelectorAll(".scroll-reveal");

        const observerOptions = {
          root: null,
          rootMargin: "0px",
          threshold: 0.1,
        };

        const observer = new IntersectionObserver((entries, observer) => {
          entries.forEach((entry) => {
            if (entry.isIntersecting) {
              entry.target.classList.add("is-visible");
              observer.unobserve(entry.target);
            }
          });
        }, observerOptions);

        scrollRevealElements.forEach((element) => {
          observer.observe(element);
        });
      });

      document.addEventListener("DOMContentLoaded", () => {
        const menuIcon = document.getElementById("menu");
        const closeIcon = document.getElementById("close");
        const navList = document.querySelector(".nav-menu");

        // Initialize icon visibility using classes
        menuIcon.classList.add('icon-visible'); // Hamburger visible initially
        closeIcon.classList.add('icon-hidden'); // Close icon hidden initially

        menuIcon.addEventListener("click", () => {
          navList.classList.add("show");
          menuIcon.classList.remove('icon-visible');
          menuIcon.classList.add('icon-hidden');
          closeIcon.classList.remove('icon-hidden');
          closeIcon.classList.add('icon-visible');
        });

        closeIcon.addEventListener("click", () => {
          navList.classList.remove("show");
          closeIcon.classList.remove('icon-visible');
          closeIcon.classList.add('icon-hidden');
          menuIcon.classList.remove('icon-hidden');
          menuIcon.classList.add('icon-visible');
        });
      });
    </script>

    <script>
      // Loading Screen JavaScript
      document.addEventListener("DOMContentLoaded", function () {
        const loadingScreen = document.getElementById("loadingScreen");
        const mainContent = document.getElementById("mainContent");

        // Ensure elements exist before proceeding
        if (!loadingScreen || !mainContent) {
          console.error("Loading screen or main content element not found!");
          return; // Exit if elements are missing to prevent errors
        }

        let loadingComplete = false; // Flag to ensure hideLoadingScreen runs only once

        // Disable scrolling during loading
        document.body.style.overflow = "hidden";
        document.documentElement.style.overflow = "hidden";

        // Set minimum loading time (in milliseconds)
        const minLoadingTime = 2000; // 2 seconds
        const startTime = Date.now();

        function hideLoadingScreen() {
          if (loadingComplete) {
            return; // Already hidden, prevent multiple executions
          }
          loadingComplete = true; // Mark as complete

          const elapsedTime = Date.now() - startTime;
          const remainingTime = Math.max(0, minLoadingTime - elapsedTime);

          setTimeout(() => {
            // Fade out loading screen
            loadingScreen.classList.add("fade-out");

            // Show main content after fade out completes
            // Use a transitionend event listener for more reliable timing
            loadingScreen.addEventListener(
              "transitionend",
              function handler() {
                loadingScreen.style.display = "none";
                mainContent.classList.add("show"); // Assuming 'show' makes it visible

                // Re-enable scrolling
                document.body.style.overflow = "auto";
                document.documentElement.style.overflow = "auto";

                // Remove the event listener to prevent it from firing again
